source 'https://rubygems.org'

gem 'dashing'

# Gem to connect to Mixpanel
gem 'mixpanel_client'

# Use http client to retrieve web resources
gem 'httpclient'

# To get access to cloudwatch data
gem 'aws-sdk'

# To get access to calendars
gem 'google-api-client'

# Add active support from rails
gem 'activesupport'

# To get access to github stats
gem 'octokit'
gem 'faraday', '~> 0.9'
gem 'faraday-http-cache'
